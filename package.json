{
    "name": "capstone-project",
    "version": "1.0.0",
    "description": "Capstone-Project",
    "engines": {
        "node": "19.7.0"
    },
    "proxy": "http://localhost:3002/",
    "scripts": {
        "backend": "cd ./backend && npm run dev",
        "client": "cd ./client && npm run dev",
        "dev": "cd ./backend && npm run dev && cd ../client && npm run dev-start",
        "start": "cd ./backend && npm run start",
        "build": "cd ./backend && npm i && cd ../client && npm i && npm run build",
        "prettier:fix-all": "prettier --write ./backend/src/**/*.{ts,tsx,js,jsx,html,scss,css,md,json} && prettier --write ./client/src/**/*.{ts,tsx,js,jsx,html,scss,css,md,json}",
        "prettier:fix-all-backend": "prettier --write ./backend/src/**/*.{ts,tsx,js,jsx,html,scss,css,md,json}",
        "prettier:fix-all-client": "prettier --write ./client/src/**/*.{ts,tsx,js,jsx,html,scss,css,md,json}",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/delcada6882/Capstone-project.git"
    },
    "keywords": [
        "Mtec",
        "Capstone",
        "Root"
    ],
    "author": "Cole & Adam",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/delcada6882/Capstone-project/issues"
    },
    "homepage": "https://github.com/delcada6882/Capstone-project#readme",
    "devDependencies": {
        "prettier": "2.8.7"
    }
}
